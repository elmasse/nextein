{
  "name": "nextein",
  "version": "1.0.0",
  "description": "A static site and blog generator based on Next.js",
  "repository": "https://github.com/elmasse/nextein",
  "bugs": "https://github.com/elmasse/nextein/issues",
  "homepage": "https://nextein.now.sh",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "taskr",
    "release": "taskr release",
    "prepublish": "npm run release",
    "lint": "standard 'bin/*' 'src/**/*.js' "
  },
  "bin": {
    "nextein": "./dist/bin/nextein"
  },
  "files": [
    "dist",
    "config.js",
    "link.js",
    "posts.js",
    "post.js"
  ],
  "dependencies": {
    "chokidar": "2.0.0",
    "cross-spawn": "^5.1.0",
    "frontmatter": "0.0.3",
    "glob": "^7.1.2",
    "minimist": "^1.2.0",
    "object.assign": "^4.0.4",
    "path-match": "^1.2.4",
    "path-to-regexp": "2.1.0",
    "pkg-up": "^2.0.0",
    "rehype-raw": "2.0.0",
    "rehype-react": "3.0.2",
    "remark-parse": "4.0.0",
    "remark-rehype": "3.0.0",
    "uglifyjs-webpack-plugin": "1.1.5",
    "unfetch": "^3.0.0",
    "unified": "6.1.6",
    "unist-util-select": "^1.5.0"
  },
  "peerDependencies": {
    "next": "^4.0.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "devDependencies": {
    "@taskr/babel": "1.1.0",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/watch": "1.1.0",
    "babel-core": "^6.24.1",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "jest": "22.0.1",
    "next": "4.2.1",
    "react-dom": "16.2.0",
    "react": "16.2.0",
    "react-test-renderer": "16.2.0",
    "standard": "10.0.3",
    "taskr": "1.1.0",
    "taskr-standard": "1.0.1"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ],
    "testRegex": "(/^test/.*|\\.(test|spec))\\.(js)$"
  },
  "keywords": [
    "blog",
    "blog-engine",
    "static",
    "website",
    "next.js",
    "react",
    "markdown",
    "ssr"
  ],
  "author": {
    "name": "Maximiliano Fierro",
    "email": "elmasse@gmail.com"
  },
  "license": "ISC"
}
