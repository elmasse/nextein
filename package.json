{
  "name": "nextein",
  "version": "4.1.1",
  "description": "A static site and blog generator based on Next.js",
  "repository": "https://github.com/elmasse/nextein",
  "bugs": "https://github.com/elmasse/nextein/issues",
  "homepage": "https://nextein.elmasse.io",
  "scripts": {
    "clean": "rimraf -rf ./dist",
    "compile": "babel ./src -d dist",
    "build": "npm run lint & npm run compile",
    "lint": "standard",
    "prepublishOnly": "npm run release",
    "release": "npm run clean && npm run build",
    "test": "jest",
    "version": "chan release ${npm_package_version} && git add ."
  },
  "files": [
    "dist",
    "create.js",
    "config.js",
    "content.js",
    "fetcher.js",
    "filters.js",
    "index.js",
    "sorters.js"
  ],
  "dependencies": {
    "@babel/runtime": "7.18.9",
    "chokidar": "^3.5.3",
    "frontmatter": "^0.0.3",
    "mime": "^3.0.0",
    "path-to-regexp": "^6.2.1",
    "rehype-raw": "^6.1.1",
    "rehype-react": "^7.1.1",
    "rehype-stringify": "^9.0.3",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-remove-position": "^4.0.1",
    "unist-util-select": "^4.0.1"
  },
  "peerDependencies": {
    "next": "^9.5.0 || ^10.0.0 || ^11.0.0 || ^12.0.0",
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.18.10",
    "@babel/core": "^7.18.0",
    "@babel/eslint-parser": "7.18.9",
    "@babel/plugin-proposal-class-properties": "^7.18.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-react": "^7.18.0",
    "@babel/runtime": "^7.18.0",
    "@geut/chan": "^3.2.9",
    "babel-jest": "^29.0.0-alpha.5",
    "eslint": "^8.21.0",
    "eslint-config-standard": "^17.0.0",
    "hastscript": "^7.0.0",
    "jest": "^29.0.0-alpha.5",
    "next": "^12.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "rimraf": "3.0.2",
    "standard": "^17.0.0",
    "webpack": "^5.74.0"
  },
  "standard": {
    "parser": "@babel/eslint-parser",
    "ignore": [
      "test/**",
      "**/node_modules/**"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ],
    "testRegex": "(/^test/.*|\\.(test|spec))\\.(js)$",
    "transform": {
      ".js": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(unified|bail|is-plain-obj|trough|vfile.*|unist.+|remark.+|rehype.+|mdast.+|hast.*|micromark.+|decode-named-character-reference|character-entities|trim-lines|property-information|.+-separated-tokens|web-namespaces|zwitch|html.*)/)"
    ]
  },
  "keywords": [
    "blog",
    "blog-engine",
    "static",
    "website",
    "next.js",
    "react",
    "markdown",
    "ssr"
  ],
  "author": {
    "name": "Maximiliano Fierro",
    "email": "elmasse@gmail.com"
  },
  "license": "ISC",
  "browserslist": "> 2%, not dead"
}
