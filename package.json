{
  "name": "nextein",
  "version": "3.0.0-beta.30",
  "description": "A static site and blog generator based on Next.js",
  "repository": "https://github.com/elmasse/nextein",
  "bugs": "https://github.com/elmasse/nextein/issues",
  "homepage": "https://nextein.elmasse.io",
  "main": "index.js",
  "scripts": {
    "build": "taskr build",
    "build:watch": "taskr watch",
    "lint": "taskr lint",
    "postinstall": "opencollective-postinstall || true",
    "prepublishOnly": "npm run release",
    "release": "taskr release",
    "test": "jest",
    "version": "chan release ${npm_package_version} && git add ."
  },
  "bin": {
    "nextein": "./dist/bin/nextein"
  },
  "files": [
    "dist",
    "config.js",
    "content.js",
    "fetcher.js",
    "filters.js",
    "link.js",
    "posts.js",
    "post.js"
  ],
  "dependencies": {
    "chokidar": "^3.4.2",
    "cross-spawn": "^7.0.0",
    "frontmatter": "^0.0.3",
    "hoist-non-react-statics": "3.3.2",
    "mime": "^2.4.6",
    "minimist": "^1.2.0",
    "object.assign": "^4.0.4",
    "opencollective-postinstall": "2.0.3",
    "path-match": "^1.2.4",
    "path-to-regexp": "^6.0.0",
    "pkg-up": "^3.0.0",
    "react-display-name": "^0.2.4",
    "rehype-parse": "^6.0.0",
    "rehype-raw": "^4.0.1",
    "rehype-react": "^5.0.0",
    "rehype-stringify": "^7.0.0",
    "remark-parse": "8.0.2",
    "remark-rehype": "^6.0.0",
    "unfetch": "^4.0.0",
    "unified": "^9.0.0",
    "unist-util-remove-position": "^2.0.0",
    "unist-util-select": "^3.0.0"
  },
  "peerDependencies": {
    "next": "^9.5.0 || ^10.0.0",
    "react": "^16.8.0 || ^17.0.0",
    "react-dom": "^16.8.0 || ^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.0",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.1",
    "@babel/runtime": "7.9.0",
    "@geut/chan": "2.2.0",
    "@taskr/babel": "1.1.0-next.2",
    "@taskr/clear": "1.1.0",
    "@taskr/esnext": "1.1.0",
    "@taskr/watch": "1.1.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint-config-standard": "^14.0.0",
    "hastscript": "^5.1.0",
    "jest": "^24.8.0",
    "next": "^10.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-test-renderer": "^17.0.0",
    "standard": "^14.3.1",
    "taskr": "1.1.0",
    "taskr-standard": "^1.2.0"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "**/node_modules/**"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "test/"
    ],
    "testRegex": "(/^test/.*|\\.(test|spec))\\.(js)$"
  },
  "keywords": [
    "blog",
    "blog-engine",
    "static",
    "website",
    "next.js",
    "react",
    "markdown",
    "ssr"
  ],
  "author": {
    "name": "Maximiliano Fierro",
    "email": "elmasse@gmail.com"
  },
  "license": "ISC",
  "collective": {
    "type": "opencollective",
    "url": "https://opencollective.com/nextein"
  }
}
